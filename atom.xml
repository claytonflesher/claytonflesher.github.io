---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <Title>Clayton Flesher</title>
  <link href="http://calvyn82.github.io/atom.xml" rel="self"/>
  <link href="http://calvyn82.github.io/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://calvyn82.github.io/</id>
  <author>
    <name>Clayton Flesher</name>
    <email>claytonflesher@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://calvyn82.github.io{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://calvyn82.github.io{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
